{
  "expo": {
    "name": "SENA Mobile App",
    "slug": "sena-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "sena-mobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Background.png",
      "resizeMode": "contain",
      "backgroundColor": "#6AB344"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sena.mobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#6AB344"
      },
      "package": "com.sena.mobileapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "jsEngine": "hermes",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Background.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6AB344"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "VITE_API_BASE_URL": "http://192.168.1.8:8081/api/",
      "eas": {
        "projectId": "7f9a23ea-0ad8-455a-9b95-4de890f81088"
      }
    }
  }
}
